{
  "productName": "viewer",
  "version": "0.1.0",
  "identifier": "com.lion.pcbviewer",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:5100",
    "beforeDevCommand": "pnpm next-start",
    "beforeBuildCommand": "pnpm next-build"
  },
  "app": {
    "windows": [
      {
        "title": "viewer",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
